<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="my_robot">
  <static>false</static>
  <link name='chassis'>
            <pose>0 0 .1 0 0 0</pose>

            <collision name='collision'>
              <geometry>
                <box>
                  <size>.3 .2 .1</size>
                </box>
              </geometry>
            </collision>

            <visual name='visual'>
              <geometry>
                <box>
                  <size>.3 .2 .1</size>
                </box>
              </geometry>
            </visual>
          </link>
        

      
      
<link name="front_wheel">
        <pose>-0.3 0 0.1 0 1.5707 1.5707</pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </visual>
      </link>
      
 <link name='front_wheel_hinge'>
            <pose>-0.3 .05 .1 0 0 0</pose>

            <collision name='collision'>
              <geometry>
                <box>
                  <size>.05 .05 .05</size>
                </box>
              </geometry>
            </collision>

            <visual name='visual'>
              <geometry>
                <box>
                  <size>.05 .05 .05</size>
                </box>
              </geometry>
            </visual>
          </link>
 	      
 	<link name="left_wheel">
        <pose>.1 .13 .1 0 1.5707 1.5707</pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

      <link name="right_wheel">
        <pose>.1 -.13 .1 0 1.5707 1.5707</pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

<joint type="revolute" name="left_wheel_hinge">
        <pose>0 0 -0.03 0 0 0</pose>
        <child>left_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
      </joint>

      <joint type="revolute" name="right_wheel_hinge">
        <pose>0 0 0.03 0 0 0</pose>
        <child>right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
      </joint>

<joint name="propulsion_joint" type="revolute">
       <pose>0 0 0.1 0 0 0</pose>
       <parent>front_wheel_hinge</parent>
       <child>front_wheel</child>
       <axis> 
       <xyz>0 -1 0</xyz>
       </axis>
       <limit>
            <velocity>1000</velocity>
        </limit>
</joint>     


<joint name="steering_joint" type="revolute">
	<pose>0 -0.05 0.1 0 0 0</pose>
       <parent>chassis</parent>
       <child>front_wheel_hinge</child>
       <axis> 
       <xyz>0 0 -1</xyz>
       </axis>
       <limit>
            <lower>-.8</lower>
            <upper>.8</upper>
            <velocity>1000</velocity>
       </limit>
</joint>

 
      
  </model>
</sdf>
